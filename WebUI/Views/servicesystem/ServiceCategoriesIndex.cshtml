@using Inv.WebUI.Resources
@{
    ViewBag.Title = @SystemResource.Men_AccTrReceiptNote;
    Layout = "~/Views/Shared/Layout.cshtml";
}
<style>
    .class_btn_drop {
        background-color: #c8c8c8;
        padding: 6px 10px 2px 10px;
    }

    .dropdown-item:hover {
        cursor: pointer !important;
        background-color: #c8c8c8 !important;
    }

    .pading_3 {
        padding: 0px 50px 0px !important
    }

    .display_none {
        display: none !important;
    }

    .textalign {
        text-align: center
    }

    .src-btn {
        border-radius: 18px !important;
    }

    .dropdown-menu {
        border-radius: 11px;
    }

    .disabledDiv {
        pointer-events: none;
        opacity: 0.4;
    }

    #Div_control label {
        width: 10% !important
    }
</style>
<script>
    $(document).ready(function () {
        $("#txt_D_CashBox").select2();
        $('#but_read').click(function () {
            var username = $('#txt_D_CashBox option:selected').text();
            var userid = $('#txt_D_CashBox').val();
            $('#result').html("id : " + userid + ", name : " + username);
        });
    });
</script>
<div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 margingred  borderred">
    <div class="row">
        <div id="informtionReserved" class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12  ">
            <div id="id_div_Add">
                <div id="id_ReportGrid" class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12  margingred" style="">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xl-3 col-xs-3 " style="margin-bottom: 4px;margin-right: 27px;margin-top: 9px;">
                            <input id="searchbutmemreport" name="" type="text" class="form-control input-sm  text_Display" placeholder="@SystemResource.App_SearchbyCode">
                        </div>
                        <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1  marginstenig pading_lep  pading_ServiceCat text-end">  @SystemResource.Purchase   </label>
                        <div class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1 " style="width:5%">
                            <input type="radio" class="form-control input-sm input-sm addable editable" name="Type" id="TypeS">
                        </div>
                        <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1  marginstenig pading_lep  pading_ServiceCat text-end">@SystemResource.Men_Sales  </label>
                        <div class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1 " style="width:5%">
                            <input type="radio" class="form-control input-sm input-sm addable editable" name="Type" id="TypeP">
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-4 col-xl-4 col-xs-4">
                        </div>
                        <div class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1 ">
                            <button type="button" class="btn btn-success" id="btnAdd">
                                @SystemResource.App_Add  <span class="glyphicon glyphicon-plus-sign"></span>
                            </button>
                        </div>
                    </div>
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
                        <table id="ReportGrid"></table>
                    </div>
                </div>
            </div>
            <h4 style="text-align: center;font-weight: bold;color: #0088c7;"><i class="glyphicon glyphicon-hand-up"></i> @SystemResource.App_DoubleDisplay <i class="glyphicon glyphicon-hand-up"></i></h4>

            <div id="DivMassage" class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12  margingred  borderred  display_none">
                <h4 id="Text_Massage" style=""> @SystemResource.App_Message </h4>
            </div>
            <div id="Div_control" class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 display_none ">
                <div class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2 float_left_right style_btton2 ">
                    <button type="button" class="btn btn-primary float_left_right " id="btnedite">@SystemResource.App_Update <span class="glyphicon glyphicon-edit"></span></button>
                    <button id="btnsave" type="button" class=" btn btn-success display_none">@SystemResource.App_save  <span class="glyphicon glyphicon-floppy-saved"></span></button>
                    <button id="btnback" type="button" class=" btn btn-warning display_none">@SystemResource.App_back  <span class="glyphicon glyphicon-backward"></span></button>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 ">
                    <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1  marginstenig pading_lep  pading_ServiceCat">@SystemResource.App_Code</label>
                    <div class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2 ">
                        <input id="txt_CODE" disabled name="CAT_CODE" type="number" class="form-control input-sm  text_Display" placeholder="@SystemResource.App_Number">
                    </div>
                    <label class="col-lg-1 col-md-2 col-sm-2 col-xl-2 col-xs-2   marginstenig pading_lep  pading_ServiceCat">@SystemResource.App_DescA </label>
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xl-3 col-xs-3 " style="left:1%">
                        <input id="txt_descA" disabled name="DescA" type="text" class="form-control input-sm  text_Display" placeholder="@SystemResource.App_DescA">
                    </div>
                    <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1   marginstenig pading_lep  pading_ServiceCat">@SystemResource.App_DescE </label>
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xl-3 col-xs-3 ">
                        <input id="txt_descE" disabled name="DescE" type="text" class="form-control input-sm  text_Display" placeholder="@SystemResource.App_DescE">
                    </div>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 ">
                    <label id="lebtxtSaleNO" class="col-lg-1 col-md-2 col-sm-2 col-xl-2 col-xs-2  w marginstenig pading_lep  pading_ServiceCat"> @SystemResource.PurAccNo </label>
                    <div class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2 ">
                        <button type="button" class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2 src-btn btn btn-warning input-sm p-0" id="btnSaleNO" name="ColSearch" disabled="disabled">
                            <i class="fa fa-search"></i>
                        </button>
                        <input id="txtSaleNO" name="SALES_ACC_CODE" disabled="disabled" type="text" class="col-lg-10 col-md-10 col-sm-10 col-xl-10 col-xs-10 pl-0  form-control input-sm  text_Display">
                    </div>
                    <input id="txtSaleD" name="ACC_DESCA" disabled="disabled" type="text" class="col-lg-4 col-md-4 col-sm-4 col-xl-4 col-xs-4 pl-0  form-control input-sm  text_Display">
                    <label style="margin-right:1%!important" class="col-lg-1 col-md-2 col-sm-2 col-xl-2 col-xs-2  marginstenig pading_lep  pading_ServiceCat"> @SystemResource.vat_natuer</label>
                    <div class="col-lg-3 col-md-3 col-sm-2 col-xl-2 col-xs-2">
                        <select id="txtvat_natuer" disabled name="VatNatID" class="form-control input-sm  text_Display "></select>
                    </div>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 ">
                    <label class="col-lg-1 col-md-2 col-sm-2 col-xl-2 col-xs-2  marginstenig pading_lep  pading_ServiceCat">@SystemResource.return_accno </label>
                    <div class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2 ">

                        <button type="button" class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2 src-btn btn btn-warning input-sm p-0" id="btnreturnNO" name="ColSearch" disabled="disabled">
                            <i class="fa fa-search"></i>
                        </button>
                        <input id="txtreturnNO" name="RETURN_ACC_CODE" disabled="disabled" type="text" class="col-lg-10 col-md-10 col-sm-10 col-xl-10 col-xs-10 pl-0  form-control input-sm  text_Display">
                    </div>
                    <input id="txtreturnD" name=" " disabled="disabled" type="text" class="col-lg-4 col-md-4 col-sm-4 col-xl-4 col-xs-4 pl-0  form-control input-sm  text_Display">

                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 ">
                    <label class="col-lg-1 col-md-2 col-sm-2 col-xl-2 col-xs-2  marginstenig pading_lep  pading_ServiceCat">@SystemResource.discount_accno </label>
                    <div class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2">

                        <button type="button" class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2 src-btn btn btn-warning input-sm p-0" id="btndiscountNO" name="ColSearch" disabled="disabled">
                            <i class="fa fa-search"></i>
                        </button>
                        <input id="txtdiscountNO" name="DISC_ACC_CODE" disabled="disabled" type="text" class="col-lg-10 col-md-10 col-sm-10 col-xl-10 col-xs-10 pl-0  form-control input-sm  text_Display">
                    </div>
                    <input id="txtdiscountD" name="" disabled="disabled" type="text" class="col-lg-4 col-md-4 col-sm-4 col-xl-4 col-xs-4 pl-0  form-control input-sm  text_Display">
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 ">

                    <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1  marginstenig pading_lep  pading_ServiceCat">@SystemResource.Service_prefix  </label>
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xl-3 col-xs-3 ">
                        <input id="txtService_prefix" disabled name="ItemFormatFix" type="text" class="form-control input-sm  text_Display" placeholder="@SystemResource.Service_prefix">
                    </div>
                    <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1  marginstenig pading_lep  pading_ServiceCat">@SystemResource.Service_serial</label>
                    <div class="col-lg-3 col-md-3 col-sm-3 col-xl-3 col-xs-3 ">
                        <input id="txtService_serial" disabled name="ItemFormatSerial" type="text" class="form-control input-sm  text_Display" placeholder="@SystemResource.Service_serial">
                    </div>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
                    <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1  marginstenig pading_lep  pading_ServiceCat  borderbottom  "> @SystemResource.App_Create </label>
                    <input id="txtCreatedBy" name="CreatedBy" type="text" disabled class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1  marginstenig pading_lep  pading_ServiceCat  borderbottom ">
                    <input id="txtCreatedAt" name="CreatedAt" type="text" disabled class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2   marginstenig pading_lep  pading_ServiceCat style_borderbottom">
                    <div class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2"> </div>
                    <label class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1  marginstenig pading_lep  pading_ServiceCat  borderbottom "> @SystemResource.App_Update </label>
                    <input id="txtUpdatedBy" name="UpdatedBy" type="text" disabled class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1  marginstenig pading_lep  pading_ServiceCat  borderbottom">
                    <input id="txtUpdatedAt" name="UpdatedAt" type="text" disabled class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2   marginstenig pading_lep  pading_ServiceCat style_borderbottom">
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/ClientApp/servicesystem/ServiceCategories.js"></script>
<script>
    $('#lepnotifications').on('click', function () {
        $('#divnotifications').toggleClass('showdiv');
        $('#spanlepnotifications').toggleClass('fa-caret-left');
        $('#spanlepnotifications').toggleClass('fa-caret-down');
    });
</script>
