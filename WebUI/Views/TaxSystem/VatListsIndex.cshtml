@using Inv.WebUI.Resources
@{
    Layout = "~/Views/Shared/Layout.cshtml"; }
<style>
    .display_none {
        display: none;
    }
    input[type=checkbox] {
        margin: -1px 0 0 !important;
    }
    .font {
        font-size: larger;
    }

    .font2 {
        font-size: 25px;
    }

    .border_gridCont {
        margin-top: 0%;
    }

    .text-center {
        text-align: center !important;
    }

    input[type=radio] {
        height: 25px !important;
    }

    .inputvat {
        background: url('data:image/svg+xml;utf8,% width="40" height="30"><text x="5" y="19" style="font: bold 16px Arial;">Age:</text></svg>') no-repeat;
        border: 1px solid #555;
        box-sizing: border-box;
        padding-left: 50px;
    }

    .input-box {
        position: relative;
    }

    .input {
        display: block;
        border: 1px solid #d7d6d6;
        background: #fff;
        padding: 10px 10px 10px 20px;
        width: 195px;
    }

    .unit {
        position: absolute;
        display: block;
        left: 18px;
        top: 10px;
        z-index: 9;
        font-weight: bold;
    }
</style>
<div id="content_report" class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12   borderred ">
    <div class="col-xs-6">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
            <label for="" class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2 pading_2 textcentr_padingVatSettingVatSetting"> @SystemResource.App_FromDate </label>
            <div class="col-lg-4 col-md-4 col-sm-4 col-xl-4 col-xs-4">
                <input type="date" class="input-sm form-control " name="" id="txtStartDate">
            </div>
            <label for="txtToDate" class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2 pading_2 textcentr_padingVatSettingVatSetting"> @SystemResource.App_ToDate  </label>
            <div class="col-lg-4 col-md-4 col-sm-4 col-xl-4 col-xs-4">
                <input type="date" class="input-sm  form-control " name=" " id="txtEndDate">
            </div>
        </div>
        <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
            <label class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2 pading_lep  pading_2 textcentr_padingVatSettingVatSetting ">@SystemResource.App_Branch</label>
            <div class="col-lg-3 col-md-3 col-sm-3 col-xl-3 col-xs-3 ">
                <input id="txtbranchCode" type="text" autofocus="autofocus" class="form-control input-sm   ">
            </div>
            <div class="col-lg-1 col-md-1 col-sm-1 col-xl-1 col-xs-1">
                <button type="button" class=" src-btn btn btn-search input-sm" id="btnBranchSearch" name="ColSearch">
                    <i class="fa fa-search"></i>
                </button>
            </div>
            <div class="col-lg-6 col-md-6 col-sm-6 col-xl-6 col-xs-6 p-0">
                <input id="txtbranchDesc" type="text" autofocus="autofocus" class="form-control input-sm   " disabled="disabled">
            </div>
        </div>
        <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
            <label for="" class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2 pading_2 textcentr_padingVatSettingVatSetting">@SystemResource.App_System</label>
            <div class="col-lg-4 col-md-4 col-sm-4 col-xl-4 col-xs-4">
                <select id="drpSystem" name="" class="form-control input-sm text_Display"></select>
            </div>
            <label class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2 pading_2 textcentr_padingVatSettingVatSetting">@SystemResource.Movment</label>
            <div class="col-lg-4 col-md-4 col-sm-4 col-xl-4 col-xs-4 ">
                <select id="drpTrans" name="" class="form-control input-sm text_Display"></select>
            </div>
        </div>
        <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
            <label for="" class="col-lg-2 col-md-2 col-sm-2 col-xl-2 col-xs-2 pading_2 textcentr_padingVatSettingVatSetting">@SystemResource.App_TaxType</label>
            <div class="col-lg-4 col-md-4 col-sm-4 col-xl-4 col-xs-4">
                <select id="drpSalesVatTp" name="" class="form-control input-sm text_Display"></select>
                <select id="drpPurVatTp" name="" class="form-control input-sm text_Display display_none"></select> 
            </div>
           



        </div>



        @*//////*@
        <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 border_gridCont" id="">
                 <div class="col-xs-3 ">
                    <div class=" col-xs-8 p-0" style="font-weight:bold">
                        <label class=" col-xs-9 pading_2 pr-0 pl-0"> @SystemResource.Summary  </label>
                        <input class="input-sm form-control col-xs-3  pr-0 pl-0" type="radio" checked="checked" id="rd_SummeryRpt" name="ReportTp" value="2">
                    </div>
                </div>
                <div class="col-xs-3 ">
                    <div class=" col-xs-8 p-0" style="font-weight:bold">
                        <label class=" col-xs-9 pading_2 pr-0 pl-0">@SystemResource.Detailed</label>
                        <input class="input-sm form-control col-xs-3  pr-0 pl-0" type="radio" id="rd_DetailRpt" name="ReportTp" value="2">
                    </div>
                </div>
            <div class="col-lg-4 col-md-4 col-sm-4 col-xl-4 col-xs-4 display_none" id="div_Landscape">
                <label for="" class="col-xs-6 pading_2 ">@SystemResource.width_Print</label>
                <input id="chkLandscapePrint" type="checkbox" autofocus="autofocus" class="form-control input-sm col-xs-6">
            </div>
        </div>




    </div>
    <div class="col-xs-6">
        <div class="col-lg-4 col-md-4 col-sm-4 col-xl-4 col-xs-4">

            <div class="6col-xs-6 float-left">
                <div class="btn-group ">
                    <button type="button" class="btn btn-warning dropdown-toggle" data-toggle="dropdown">
                        @SystemResource.App_Print  <span class="glyphicon glyphicon-hand-down"></span>
                    </button>
                    <div class="dropdown-menu" role="menu" style="border-radius:15px">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 display_none">
                            <button style="width:100%" id="btnPrint" type="button" class="  btn btn-custon-four btndefolt primary-btn-cl"><span class="adminpro-icon adminpro-paint-roller"></span>           @SystemResource.App_Print      </button>
                        </div>
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
                            <button style="width:100%" id="btnPrintTrview" type="button" class="  btn btn-custon-four btndefolt primary-btn-cl"><span class="adminpro-icon adminpro-paint-roller"></span>           @SystemResource.Print_Report      </button>
                        </div>
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
                            <button style="width:100%" id="btnPrintTrPDF" type="button" class="marginbtn btn btn-custon-four btndefolt primary-btn-cl"><span class="adminpro-icon adminpro-download-arrow"></span>   @SystemResource.Download_PDF     </button>
                        </div>

                        <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12">
                            <button style="width:100%" id="btnPrintTrEXEL" type="button" class="marginbtn btn btn-custon-four btndefolt primary-btn-cl"><span class="adminpro-icon adminpro-download-arrow"></span> @SystemResource.Download_EXCEL   </button>
                        </div>

                    </div>
                </div>
                <button id="btnAddFreeReturn" type="button" class="btn btn-warning display_none">
                    <span class="glyphicon glyphicon-save"></span> @SystemResource.btn_AddFreeReturn
                </button>
            </div>

        </div>
        <div class="col-lg-6 col-md-12 col-sm-12 col-xl-12 col-xs-12 border_gridCont">

            <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 ">
                <div class=" col-xs-8 p-0" style="font-weight:bold">
                    <label class=" col-xs-9 pading_2 pr-0 pl-0">@SystemResource.Sales_Invoice</label>
                    <input class="input-sm form-control col-xs-3  pr-0 pl-0" type="radio" id="rd_SalesInvoice" checked="checked" name="Group1" value="2">
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 ">
                <div class=" col-xs-8 p-0" style="font-weight:bold">
                    <label class=" col-xs-9 pading_2 pr-0 pl-0">@SystemResource.Update_Sales</label>
                    <input class="input-sm form-control col-xs-3  pr-0 pl-0" type="radio" id="rd_SalesUpdates" name="Group1" value="2">
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 ">
                <div class=" col-xs-8 p-0" style="font-weight:bold">
                    <label class=" col-xs-9 pading_2 pr-0 pl-0">@SystemResource.Purshase_Invoice</label>
                    <input class="input-sm form-control col-xs-3  pr-0 pl-0" type="radio" id="rd_PurchaseInvoice" name="Group1" value="2">
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 ">
                <div class=" col-xs-8 p-0" style="font-weight:bold">
                    <label class=" col-xs-9 pading_2 pr-0 pl-0">@SystemResource.Update_Purshase</label>
                    <input class="input-sm form-control col-xs-3  pr-0 pl-0" type="radio" id="rd_PurchaseUpdates" name="Group1" value="2">
                </div>
            </div>
        </div>

        
       
        <div class="col-lg-4 col-md-4 col-sm-4 col-xl-4 col-xs-4">


        </div>
        <div id="div_summery" class="col-lg-6 col-md-12 col-sm-12 col-xl-12 col-xs-12 border_gridCont ">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 ">
                <div class=" col-xs-8 p-0" style="font-weight:bold">
                    <label class=" col-xs-9 pading_2 pr-0 pl-0">@SystemResource.Collect_Benf</label>
                    <input class="input-sm form-control col-xs-3  pr-0 pl-0" type="radio" checked="checked" id="rd_Customer" name="Type" value="2">
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 ">
                <div class=" col-xs-8 p-0" style="font-weight:bold">
                    <label class=" col-xs-9 pading_2 pr-0 pl-0">@SystemResource.Collect_Process</label>
                    <input class="input-sm form-control col-xs-3  pr-0 pl-0" type="radio" id="rd_Process" name="Type" value="2">
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12 ">
                <div class=" col-xs-8 p-0" style="font-weight:bold">
                    <label class=" col-xs-9 pading_2 pr-0 pl-0">@SystemResource.Collect_branch</label>
                    <input class="input-sm form-control col-xs-3  pr-0 pl-0" type="radio" id="rd_branch" name="Type" value="2">
                </div>
            </div>
        </div>

    </div>





        @*//*****************new*@
        <div id="DivMassage" class="col-lg-12 col-md-12 col-sm-12 col-xl-12 col-xs-12  margingred  borderred display_none">

            <h4 id="Text_Massage" style=""> @SystemResource.App_Message </h4>

        </div>
        @*//**************@
    </div>
<script src="~/ClientApp/TaxSystem/VatLists.js"></script>